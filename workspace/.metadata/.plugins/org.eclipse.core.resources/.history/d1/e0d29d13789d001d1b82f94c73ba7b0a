package classTest;

public class User {
	String name;
	String phone;
	double count;
	int dis;
	public User(String name, String phone, int count, int dis) {
		super();
		this.name = name;
		this.phone = phone;
		this.count = count;
		this.dis = dis;
	}
	
	void buy(Product p) {
		count = count - ((p.price)*dis*0.01);
	}
}
